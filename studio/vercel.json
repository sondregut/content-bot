{
  "version": 2,
  "builds": [
    { "src": "server.mjs", "use": "@vercel/node" },
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "server.mjs" },
    { "src": "/brands/(.*)", "dest": "server.mjs" },
    { "src": "/(.*)", "dest": "public/$1" }
  ]
}
